<script>
function gtag_report_conversion(url) {
  var callback = function () {
    if (typeof(url) != 'undefined') {
      window.location = url;
    }
  };
  {
  gtag('event', 'conversion', {
      'send_to': 'AW-934209236/UwL7CO7CqGMQ1M27vQM',
      'event_callback': callback
  }),
  gtag('event', 'conversion', {
      'send_to': 'AW-957251125/EOEcCOrt6eQBELX8ucgD',
      'event_callback': callback
  })};
  return false;
}
</script>

